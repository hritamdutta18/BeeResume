    "url": "https://github.com/hritamdutta18/bee-resume/issues"
  },
  "homepage": "https://github.com/hritamdutta18/bee-resume#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^6.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "html-pdf": "^3.0.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.9"
  }
}
